{
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    }
  ]
    "headers": [
    {
      "source": "/static/(.*).(css|js|png|jpg|jpeg|gif|svg|woff2?)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"   // Or appropriate MIME type
        }
      ]
    }
  ]
}
